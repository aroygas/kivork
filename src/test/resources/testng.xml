<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Kivork test" preserve-order="true" verbose="1">
    <parameter name="url" value="https://freegeoip.app/"/>
    <parameter name="longitude" value="41.6477291292777"/>
    <parameter name="latitude" value="41.64284118816131"/>
    <test name="web" verbose="1" preserve-order="true">
        <groups>
            <run>
                <include name = "web"/>
            </run>
        </groups>
        <classes>
            <class name="com.aroygas.kivork.KivorkTest"/>
        </classes>
    </test>
    <test name="api" verbose="1" preserve-order="true">
        <groups>
            <run>
                <include name = "api"/>
            </run>
        </groups>
        <classes>
            <class name="com.aroygas.kivork.KivorkTest"/>
        </classes>
    </test>
</suite>